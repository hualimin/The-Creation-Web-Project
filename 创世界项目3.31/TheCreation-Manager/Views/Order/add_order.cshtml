
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>add_order</title>
</head>
<body>
    <div>
        <h2>添加订单</h2>
        <form action="@Url.Action("add_order_p","Order")" name="form" method="post">
            用户ID：<input type="text" name="uid" />
            创作者ID：<input type="text" name="cid" />
            订单名称：<input type="text" name="name" />
            交付日期：<input type="datetime" name="finishdate" />
            总价：<input type="text" name="tot" />
            描述：<input type="text" name="describe" />
            <button type="submit">提交</button>
        </form>
    </div>
</body>
</html>
